	// Assign STATIC_AREA_ADDRESS to register "r0"
 	 loadI 1024 	=> r0 
	// Compute address of variable "a" at offset 0 in register 2
 	 loadI 0 	=> r1 
 	 add r0, r1 	=> r2 
	// Load RHS value of variable "b" at offset 4
 	 loadAI r0, 4 	=> r3 
 	 loadI 1 	=> r4 
 	 add r3, r4 	=> r5 
 	 store r5 	=> r2 
